{
  "applications":
  {
    "gecko":
    {
      "strict_min_version": "60.0a1"
    }
  },

  "author": "Sukesan",

  "background":
  {
    "scripts": ["main.js"]
  },

  "default_locale": "en",

  "description": "__MSG_extensionDescription__",

  "icons":
  {
    "48": "icons/quickTranslator_icon_48px.png",
    "96": "icons/quickTranslator_icon_96px.png"
  },

  "manifest_version": 2,

  "name": "Quick translator",

  "options_ui":
  {
    "browser_style": true,
    "open_in_tab":   true,
    "page": "documents/options/option_page.html"
  },

  "page_action":
  {
    "browser_style": true,
    "default_icon":  "icons/quickTranslator_icon_96px.png",
    "default_title": "__MSG_tooltipTitle__",
    "show_matches":  ["<all_urls>"]
  },

  "permissions": ["menus", "storage", "tabs"],

  "version": "1.5.4.0"
}